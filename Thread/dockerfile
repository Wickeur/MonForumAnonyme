# Partir de ngix 
# Utiliser une image NGINX comme base
FROM nginx

# Set the working directory in the container
WORKDIR /var/www/html

# Copier les configurations NGINX dans le conteneur
COPY dist /usr/share/nginx/html

COPY . .

# Exposez le port sur lequel le service Ã©coute
EXPOSE 80

# Lancer npm run build avant docker compose build !!!!!
